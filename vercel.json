{
  "version": 2,
  "name": "ladl-backend",
  "builds": [
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/server.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["fra1"]
} 