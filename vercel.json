{
  "version": 2,
  "name": "ladl-backend",
  "functions": {
    "server.cjs": {
      "runtime": "@vercel/node@20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.cjs"
    },
    {
      "source": "/(.*)",
      "destination": "/server.cjs"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["fra1"]
} 